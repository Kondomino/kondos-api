{
  "name": "kondos-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "packageManager": "npm@10.0.0",
  "scripts": {
    "preinstall": "node -e \"const ua=process.env.npm_config_user_agent||''; if(!ua.startsWith('npm/')){console.error('Error: Use npm only. Detected '+ua); process.exit(1)}\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:integrator": "node src/integrator.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:watch": "tsc --watch",
    "migrate": "NODE_ENV=development npm run build && NODE_ENV=development sequelize-cli db:migrate",
    "migrate:undo": "NODE_ENV=development npm run build && NODE_ENV=development sequelize-cli db:migrate:undo",
    "migrate:status": "NODE_ENV=development npm run build && NODE_ENV=development sequelize-cli db:migrate:status",
    "create:kondo": "npx ts-node src/kondo/scripts/create-mock-kondo.ts",
    "create:unit": "npx ts-node src/unit/scripts/create-mock-unit.ts",
    "create:user": "npx ts-node src/user/scripts/create-mock-user.ts",
    "seed": "npm run create:kondo && npm run create:unit && npm run create:user",
    "token": "npx ts-node src/kondo/scripts/token-generator.ts",
    "test:db-config": "node scripts/test-db-config.js"
  },
  "engines": {
    "node": "24.11.0",
    "npm": "11.x"
  },
  "dependencies": {
    "@adobe/pdfservices-node-sdk": "^4.1.0",
    "@langchain/core": "^0.3.69",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/sequelize": "^4.28.20",
    "aws-sdk": "^2.1692.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.30",
    "nestjs-sequelize-seeder": "^1.1.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "sequelize-typescript-migration-lts": "^3.2.5",
    "stytch": "^12.43.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "nest-commander": "^3.13.0",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.6.2",
    "sequelize-slugify": "^1.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
